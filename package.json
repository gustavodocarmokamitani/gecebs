{
  "name": "simple-vite-react-express",
  "version": "1.0.0",
  "description": "Boilerplate to build fullstack app using react, express, vite and pg",
  "main": "src/server/index.ts",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "start": "npm run build && node src/server/index.js",
    "server": "nodemon src/server/index.js",
    "client": "vite",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "preview": "vite preview",
    "db:setup": "npx prisma migrate dev && npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "node prisma/seed.ts",
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Avi Maiti",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@mui/styles": "^6.4.0",
    "@prisma/client": "^6.2.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "celebrate": "^15.0.3",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.12",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mui-file-input": "^6.0.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "react-toastify": "^11.0.3",
    "strong-error-handler": "^5.0.15",
    "typescript": "^5.9.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@types/express": "^5.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "terser": "^5.37.0",
    "ts-node": "^10.9.2",
    "vite": "^6.0.7"
  }
}
