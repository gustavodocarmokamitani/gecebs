{
  "name": "gecebs-app",
  "version": "1.0.0",
  "description": "API e Frontend para a equipe de beisebol Gecebs",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build:server": "tsc",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server/index.js",
    "server": "nodemon src/server/index.ts",
    "client": "vite",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@types/node": "^20.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "celebrate": "^15.0.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "framer-motion": "^12.23.12",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "react-toastify": "^11.0.3",
    "strong-error-handler": "^5.0.15",
    "typescript": "^5.9.2",
    "xlsx": "^0.18.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "terser": "^5.37.0",
    "ts-node": "^10.9.2",
    "vite": "^6.0.7"
  }
}