{
  "version": 2,
  "builds": [
    {
      "src": "src/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/index.js"
    },
    {
      "src": "/",
      "dest": "dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}